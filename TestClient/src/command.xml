se>    	 </command>    	     </module>           <module name="friend">    	<command id="0x030001" name="friend_list" >	    	<request>	    		<string>user_id</string>	    	</request>	    	<response>	    		<list name="friend_list">	    			<string>id</string>	    			<string>name</string>	    			<string>sex</string>	    			<int>pet_id</int>	    			<int>pet_level</int>	    		</list>	    	</response>	    </command>	    	    	    <command id="0x030002" name="add_friend" >	    	<request>	    		<string>self_id</string>	    		<string>friend_name</string>	    	</request>	    	<response>	    		<bool>is_success</bool>	    		<string>result</string>	    	</response>	    </command>	    	    	    <command id="0x030003" name="delete_friend" >	    	<request>	    		<string>self_id</string>	    		<string>friend_id</string>	    	</request>	    	<response>	    		<bool>is_uccess</bool>	    		<string>result</string>	    	</response>	    </command>	    <command id="0x030004" name="send_mail_to_friend">	    	<request>	    		<string>user_id</string>	    		<string>friend_id</string>	    		<string>content</string>	    	</request>	    	<response>	    		<bool>is_ok</bool>	    	</response>	    </command>	    <command id="0x030005" name="giving_item_to_friend">	    	<request>	    		<string>user_id</string>	    		<string>friend_id</string>	    		<string>item_id</string>	    		<int>number</int>	    	</request>	    	<response>	    		<int>state</int><!-- 将bool改成int可以表示多种状态 0为成功 -->	    	</response>	    </command>	    <command id="0x030006" name="remand_item_from_friend">	    	<request>	    		<string>user_id</string>	    		<string>friend_id</string>	    		<string>item_id</string>	    		<int>number</int>	    	</request>            <response>            	<bool>is_ok</bool>            </response>	    </command>	    <command id="0x030007" name="challenge_friend">	    	<request>	    		<string>user_id</string>	    		<string>friend_id</string>	    		<int>map_id</int>	    		<int>type</int><!-- 1.练习赛 2.挑战赛  -->	    	</request>	    	<response>	    		<string>map_id</string>	    		<string>info</string>	    	</response>	    </command>	    <command id="0x030008" name="range_player">	        <request>	           <string>user_id</string>	           <string>filter_sex</string><!-- 性别筛选 -->	           <int>posx</int><!-- 放大10000倍 -->	           <int>posy</int>	           <int>radius</int><!-- 半径:单位米 -->	           <int>page</int><!-- 第N页 -->	           <int>count</int><!-- 显示多少 -->	        </request>	        <response>	            <list name="range_list">	        		<string>player_id</string>	        		<string>player_name</string>	        		<string>player_sex</string>	        		<int>distance</int><!-- 相距:单位米 -->	        		<int>pet_id</int>	        		<int>pet_level</int>	        	</list>	        </response>	    </command>    </module>    <module name="pve">        <command id="0x040001" name="pve_map_list">            <request>                <string>user_id</string>            </request>            <response>                <int>current_map_id</int>                <int>count_mark</int><!-- 总分数 -->                <int>count_star_level</int><!-- 总星级 -->                <int>life</int>                <int>cd</int>                <list name="map_list">                     <int>map_id</int>                     <int>mark</int>                     <int>star_level</int>                </list>            </response>        </command>        <command id="0x040002" name="pve_map_completion">            <request>            	<string>user_id</string>            	<int>map_id</int>            	<int>mark</int>            	<int>star_level</int>            	<bool>is_success</bool>            </request>            <response>            	<bool>is_ok</bool>            </response>        </command>        <command id="0x040003" name="pve_range_player">           <request>           		<string>user_id</string>	           <string>filter_sex</string><!-- 性别筛选 -->	           <int>posx</int><!-- 放大100000倍 -->	           <int>posy</int>	           <int>radius</int><!-- 半径:单位米 -->	           <int>page</int><!-- 第N页 -->	           <int>count</int><!-- 显示多少 -->           </request>           <response>           		<list name="range_player">           			<string>player_id</string>           			<string>player_name</string>           			<string>player_sex</string>           			<int>distance</int>           			<int>count_mark</int>           			<int>pet_id</int>           			<int>pet_level</int>           		</list>           </response>        </command>        <command id="0x040004" name="pve_friend">        	<request>        		<string>player_id</string>        	</request>        	<response>        		<list name="pve_friend_list">        			<string>player_id</string>        			<string>player_name</string>        			<string>player_sex</string>        			<int>pve_mark</int>        			<int>pve_star_level</int>        			<int>pet_id</int>        			<int>pet_level</int>        		</list>        	</response>        </command>        <command id="0x040005" name="buy_life">        	<request>        		<string>player_id</string>        		<int>coin_type</int><!-- 1.银币 2.金币 -->        	</request>        	<response>        		<int>state</int><!-- 0:成功 -->        	</response>        </command>        <command id="0x040006" name="pve_friend_by_mapid">        	<request>        		<string>player_id</string>        		<int>map_id</int>        	</request>        	<response>        		<list name="pve_friend_list">        			<string>player_id</string>        			<string>player_name</string>        			<string>player_sex</string>        			<int>pve_mark</int>        			<int>pve_star_level</int>        			<int>pet_id</int>        			<int>pet_level</int>        		</list>        	</response>        </command>        <command id="0x040007" name="pve_give_item">           <request>           		<string>player_id</string>           		<int>map_id</int>           		<string>item_id</string>           		<int>count</int>           </request>           <response>               <int>result</int><!--0:为成功,其他为失败  -->           </response>        </command>    </module>    <module name="pack">    	<command id="0x060001" name="pack_item_list">    		<request>    			<string>user_id</string>    		</request>    		<response>    			<list name="item_list">    				<string>item_id</string>    				<int>item_num</int>    				<byte>type</byte>    				<int>price</int>    			</list>    		</response>    	</command>    	<command id="0x060002" name="pack_item_buy">    		<request>    			<string>user_id</string>    			<string>item_id</string>    			<int>item_num</int>    		</request>    		<response>    			<int>state</int><!-- 将bool改成int可以表示更多种状态 -->    			<string>item_id</string>    			<int>count</int>    		</response>    	</command>    	<command id="0x060003" name="pack_item_use">    		<request>    			<string>user_id</string>    			<string>item_id</string>    			<int>item_num</int>    		</request>    		<response>    			<int>state</int><!-- 将bool改成int可以表示更多种状态 -->    		</response>    	</command>    </module>    <module name="rank">    	<command id="0x080001" name="pve_rank">    		<request>    			<int>page</int>    			<int>count</int>    			<bool>is_need_image</bool>    		</request>    		<response>    			<list name="rank_list_pve">    				<int>sort</int>    				<string>player_id</string>    				<string>player_name</string>    				<int>mark</int><!-- pveMark or pvpMark or singleMark or goodManMark -->    				<int>pve_star_level</int><!-- 其他模式该值可忽略 -->    				<bool>is_need_image</bool>    				<bytes>image</bytes>    				<int>pet_id</int>    				<int>pet_level</int>    			</list>    		</response>    	</command>    	    	<command id="0x0800