ring>	        		<string>player_sex</string>	        		<int>distance</int><!-- 相距:单位米 -->	        		<int>single_mark</int>	        		<int>pet_id</int>	        		<int>pet_level</int>	        	</list>	        </response>	    </command>    </module>    <module name="pet">    	<command id="0x0B0001" name="list_pet">    	<request>    		<string>player_id</string>    	</request>    	<response>    		<string>current_use_pet_id</string>    		<list name="pet_list">    			<string>pet_db_id</string><!-- 数据库id -->    			<int>pet_id</int><!-- 策划静态数据id -->    			<int>pet_level</int>    			<list name="pet_skill_list">    				<string>skill_id</string>    			</list>    		</list>    	</response>    	</command>    	<command id="0x0B0002" name="pet_uplevel">    		<request>    			<string>player_id</string>    			<string>pet_db_id</string>    		</request>    		<response>    			<bool>is_ok</bool>    		</response>    	</command>    	<command id="0x0B0003" name="pet_change">    		<request>    			<string>player_id</string>    			<string>pet_db_id</string>    		</request>    		<response>    			<bool>is_ok</bool>    		</response>    	</command>    	<command id="0x0B0004" name="current_pet_info">    		<request>    			<string>player_id</string>    		</request>    		<response>    			<string>pet_db_id</string><!-- 数据库id -->    			<int>pet_id</int><!-- 策划静态数据id -->    			<int>pet_level</int>    			<string>skill_id</string>    		</response>    	</command>    	<command id="0x0B0005" name="uplock_pet">    		<request>    			<string>player_id</string>    			<int>pet_id</int><!-- 1,2,3,4 -->    		</request>    		<response>    		   <bool>result</bool>    		   <string>desc</string>    		</response>    	</command>    </module>    <module name="shop">    	<command id="0x070001" name="shop_item_list">    	     <request></request>    	     <response>    	     	<list name="item_list">    	     		<string>item_id</string>    	     		<int>item_t